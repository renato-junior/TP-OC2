# Start time: 20:18:05 on Oct 10,2017
# Loading work.ALU
add wave -position end  sim:/ALU/clk
add wave -position end  sim:/ALU/codop
add wave -position end  sim:/ALU/operando1
add wave -position end  sim:/ALU/operando2
add wave -position end  sim:/ALU/resultado
add wave -position end  sim:/ALU/neg
add wave -position end  sim:/ALU/zero
add wave -position end  sim:/ALU/overflow
force -freeze sim:/ALU/clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/ALU/codop d10 0
# Invalid binary digit: d.
